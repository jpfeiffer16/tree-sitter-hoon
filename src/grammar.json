{
  "name": "YOUR_LANGUAGE_NAME",
  "rules": {
    "document": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_value"
      }
    },
    "_value": {
      "type": "SYMBOL",
      "name": "token"
    },
    "token": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "barcen"
        },
        {
          "type": "SYMBOL",
          "name": "luslus"
        },
        {
          "type": "SYMBOL",
          "name": "any"
        }
      ]
    },
    "barcen": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "|%"
        },
        {
          "type": "SYMBOL",
          "name": "token"
        },
        {
          "type": "STRING",
          "value": "--"
        }
      ]
    },
    "luslus": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "++"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "SYMBOL",
          "name": "token"
        }
      ]
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[a-z]+"
    },
    "any": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "\\S"
        },
        {
          "type": "PATTERN",
          "value": ".*"
        }
      ]
    },
    "comment": {
      "type": "TOKEN",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "::"
              },
              {
                "type": "PATTERN",
                "value": ".*"
              }
            ]
          }
        ]
      }
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    },
    {
      "type": "SYMBOL",
      "name": "comment"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

